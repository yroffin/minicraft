/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "skipLibCheck": true,
    "types": [
      "babylonjs",
      "babylonjs-loaders"
    ],
    "paths": {
      "os": [
        "./node_modules/os-browserify"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}